{
  "name": "react-mf",
  "version": "1.1.0",
  "description": "A react component to display molecular formulas",
  "author": {
    "name": "Daniel Kostro",
    "email": "kostro.d@gmail.com"
  },
  "files": [
    "lib",
    "lib-es6"
  ],
  "main": "lib/index.js",
  "keywords": [
    "react",
    "molecular formula"
  ],
  "module": "lib-es6/index.js",
  "scripts": {
    "build-docs": "build-storybook -o docs",
    "compile": "babel src --out-dir lib --ignore __tests__ --quiet",
    "compile-es6": "BABEL_ENV=es6 babel src --out-dir lib-es6 --ignore __tests__ --quiet",
    "eslint": "eslint src stories",
    "eslint-fix": "eslint --fix src",
    "prepare": "npm run compile && npm run compile-es6",
    "storybook": "start-storybook -p 6006",
    "test": "npm run eslint"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.4",
    "@storybook/addon-info": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/addon-links": "^4.0.4",
    "@storybook/addons": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "eslint-config-cheminfo": "^1.18.0",
    "eslint-config-neptune-react": "^2.5.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  },
  "repository": "neptunejs/react-mf",
  "license": "MIT",
  "dependencies": {
    "mf-parser": "^0.6.4",
    "prop-types": "^15.6.2"
  }
}
