{
  "name": "react-mf",
  "version": "2.0.2",
  "description": "A react component to display molecular formulae",
  "author": {
    "name": "Daniel Kostro",
    "email": "kostro.d@gmail.com"
  },
  "files": [
    "lib",
    "react-mf.d.ts"
  ],
  "main": "lib/index.cjs",
  "keywords": [
    "react",
    "molecular formula",
    "mf renderer"
  ],
  "module": "lib/index.mjs",
  "types": "react-mf.d.ts",
  "scripts": {
    "build-storybook": "storybook build",
    "compile": "vite build && cross-env VITE_BUID_CJS=1 vite build",
    "eslint": "eslint src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run compile",
    "dev": "storybook dev -p 6006",
    "test": "npm run eslint && npm run prettier",
    "prettier": "prettier --check src stories",
    "prettier-write": "prettier --write src stories"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.4",
    "@storybook/addon-links": "^7.0.4",
    "@storybook/addon-storysource": "^7.0.4",
    "@storybook/react": "^7.0.4",
    "@storybook/react-vite": "^7.0.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-zakodium": "7.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "prettier": "^2.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook": "^7.0.4",
    "vite": "^4.2.0",
    "vite-plugin-no-bundle": "^2.0.2"
  },
  "repository": "zakodium-oss/react-mf",
  "license": "MIT",
  "dependencies": {
    "mf-parser": "^2.0.0",
    "prop-types": "^15.8.1"
  },
  "prettier": {
    "arrowParens": "always",
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  },
  "volta": {
    "node": "16.19.0"
  }
}
