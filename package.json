{
  "name": "react-mf",
  "version": "2.0.1",
  "description": "A react component to display molecular formulae",
  "author": {
    "name": "Daniel Kostro",
    "email": "kostro.d@gmail.com"
  },
  "files": [
    "lib",
    "lib-es6",
    "react-mf.d.ts"
  ],
  "main": "lib/index.js",
  "keywords": [
    "react",
    "molecular formula",
    "mf renderer"
  ],
  "module": "lib-es6/index.js",
  "types": "react-mf.d.ts",
  "scripts": {
    "build-storybook": "build-storybook",
    "compile": "babel src --out-dir lib --ignore __tests__ --quiet",
    "compile-es6": "cross-env BABEL_ENV=es6 babel src --out-dir lib-es6 --ignore __tests__ --quiet",
    "eslint": "eslint src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run compile && npm run compile-es6",
    "dev": "start-storybook -p 6006",
    "test": "npm run eslint && npm run prettier",
    "prettier": "prettier --check src stories",
    "prettier-write": "prettier --write src stories"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-storysource": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@zakodium/eslint-config": "6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "repository": "zakodium-oss/react-mf",
  "license": "MIT",
  "dependencies": {
    "mf-parser": "^1.4.7",
    "prop-types": "^15.8.1"
  },
  "prettier": {
    "arrowParens": "always",
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  },
  "volta": {
    "node": "16.17.1"
  }
}
