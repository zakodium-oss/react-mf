{
  "name": "react-mf",
  "version": "1.2.0",
  "description": "A react component to display molecular formulas",
  "author": {
    "name": "Daniel Kostro",
    "email": "kostro.d@gmail.com"
  },
  "files": [
    "lib",
    "lib-es6"
  ],
  "main": "lib/index.js",
  "keywords": [
    "react",
    "molecular formula",
    "mf renderer"
  ],
  "module": "lib-es6/index.js",
  "scripts": {
    "build-docs": "build-storybook -o docs",
    "compile": "babel src --out-dir lib --ignore __tests__ --quiet",
    "compile-es6": "cross-env BABEL_ENV=es6 babel src --out-dir lib-es6 --ignore __tests__ --quiet",
    "eslint": "eslint src stories",
    "eslint-fix": "eslint --fix src",
    "prepare": "npm run compile && npm run compile-es6",
    "storybook": "start-storybook -p 6006",
    "test": "npm run eslint"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-cheminfo": "^3.0.0",
    "eslint-config-neptune-react": "^2.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.2.7",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "repository": "zakodium/react-mf",
  "license": "MIT",
  "dependencies": {
    "mf-parser": "^1.1.4",
    "prop-types": "^15.7.2"
  }
}
